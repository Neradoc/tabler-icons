---
layout: default
---
{% import "icon.njk" as m %}

<div class="container">
	<div class="box">
		<div class="mb">
			<div class="icons-list">
				{% for icon in collections.icons %}
				{% set name = icon.fileSlug %}
				<div class="icons-list-icon js-icon" title="{{ name }}" data-icon="{{ name }}">
					{{ m.icon(name) }}
				</div>
				{% endfor %}
			</div>
		</div>
	</div>
	<div class="box">
		<div class="mb">
			<div class="icons-list">
				{% for icon in collections.icons %}
				{% set name = icon.fileSlug %}
				<div class="icons-list-icon js-icon" title="{{ name }}" data-icon="{{ name }}">
					{{ m.icon(name, stroke="1.25") }}
				</div>
				{% endfor %}
			</div>
		</div>
	</div>
</div>
